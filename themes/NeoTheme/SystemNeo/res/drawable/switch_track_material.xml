<?xml version="1.0" encoding="UTF-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android" android:enterFadeDuration="300" android:exitFadeDuration="300">
    <item android:alpha="?android:disabledAlpha" android:state_checked="true" android:state_enabled="false">
        <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
            <item android:bottom="8dp" android:gravity="fill_horizontal|center" android:top="8dp">
                <shape android:shape="rectangle">
                    <solid android:color="?android:colorControlActivated"/>
                    <size android:height="2.0dip"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:alpha="?android:disabledAlpha" android:state_checked="false" android:state_enabled="false">
        <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
            <item android:bottom="8dp" android:gravity="fill_horizontal|center" android:top="8dp">
                <shape android:shape="rectangle">
                    <solid android:color="?android:colorControlNormal"/>
                    <size android:height="2.0dip"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="true" android:state_enabled="true">
        <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
            <item android:bottom="8dp" android:gravity="fill_horizontal|center" android:top="8dp">
                <shape android:shape="rectangle">
                    <solid android:color="?android:colorControlActivated"/>
                    <size android:height="2.0dip"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="false" android:state_enabled="true">
        <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
            <item android:bottom="8dp" android:gravity="fill_horizontal|center" android:top="8dp">
                <shape android:shape="rectangle">
                    <solid android:color="?android:colorControlNormal"/>
                    <size android:height="2.0dip"/>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
